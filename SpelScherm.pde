void tekenSpelScherm() {
  veranderWaardes();
  beweegRuimteMonster();
  beweegIntelligentRuimteMonster();
  tekenSpeelveld();
  tekenTekst("score: " + getScore(), scoreX, scoreY, textGrootte, #ff0000);
  if ( getHoeveelheidSpelers() == 1) {
    tekenSpeler(0);
  } else if (getHoeveelheidSpelers() == 2) {
    tekenSpeler(0);
    tekenSpeler(1);
  }
}

void veranderWaardes() {
  intelligentRuimteMonsterSpeed = intelligentRuimteMonsterMaxSpeed + 1 - getSnelheidIntelligentRuimteMonster();
  scoreY = hoogte / 2;
  scoreX = breedte;
  textGrootte = breedte / 2;
  ruimteMonsterWachtTijd ++;
  zuurstof --;
}

