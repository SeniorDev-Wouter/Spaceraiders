void genereerSpeelVeld() {
  initialiseerSpelScherm();
  resetSpeelveld();
  genereerSchatten(getHoeveelheidSchatten());
  if (level > 0) {
    genereerEntity(speelveld[level].length * 3 - 5, MUUR);
  }
  genereerEntity(getHoeveelheidBadRaiders(), BADRAIDER);
  genereerEntity(getHoeveelheidDeathTraps(), DEATHTRAP);
  genereerEntity(getHoeveelheidZuurstofTanks(), ZUURSTOFTANK);
  genereerEntity(1, INTELLIGENTRUIMTEMONSTER);
  genereerRuimteMonsters(getHoeveelheidRuimteMonsters());
}

void detectSpelerCollision() {
  for (int i = 0; i < spelers.length; i++) {
    switch(speelveld[level][getSpelerCoordinaat(i, 0)][getSpelerCoordinaat(i, 1)]) {
    case SCHATMUNT: 
      speelveld[level][getSpelerCoordinaat(i, 0)][getSpelerCoordinaat(i, 1)] = 0;
      score ++;
      break;
    case SCHATMUNTSTAPEL: 
      speelveld[level][getSpelerCoordinaat(i, 0)][getSpelerCoordinaat(i, 1)] = 0;
      score += 2;
      break;
    case SCHATGEM: 
      speelveld[level][getSpelerCoordinaat(i, 0)][getSpelerCoordinaat(i, 1)] = 0;
      score += 3;
      break;
    case SCHATKIST: 
      speelveld[level][getSpelerCoordinaat(i, 0)][getSpelerCoordinaat(i, 1)] = 0;
      score += 5;
      break;
      //println(score);
    case BADRAIDER:
      speelveld[level][getSpelerCoordinaat(i, 0)][getSpelerCoordinaat(i, 1)] = 0;
      score = score / 2;
      break;
      //println(score);
    case ZUURSTOFTANK:
      speelveld[level][getSpelerCoordinaat(i, 0)][getSpelerCoordinaat(i, 1)] = 0;
      zuurstof = getZuurstofMax();
      break;
      //println(zuurstof);
    case DEATHTRAP:
      setScherm(EINDSCHERM);
      dead = true;
      //println(scherm);
      break;
    case RUIMTEMONSTERLEFTRIGHT:
      setScherm(EINDSCHERM);
      dead = true;
      //println(scherm);
      break;
    case RUIMTEMONSTERUPDOWN:
      setScherm(EINDSCHERM);
      dead = true;
      //println(scherm);
      break;
    case INTELLIGENTRUIMTEMONSTER:
      scherm = EINDSCHERM;
      dead = true;
      //println(scherm);
      break;
    case PORTAAL:
      {
        if (level < speelveld.length) {
          level ++;
        }//println(scherm);
        setScherm(EINDSCHERM);
      }
      break;
    case KNOP:
      speelveld[level][1][speelveld[level].length - 2] = 6;
      //println(scherm);
      break;
    }
  }
}

void genereerSchatten(int hoeveel) {
  for (int i = 0; i < hoeveel; i++) {
    int random3 = int(random(4));
    switch(random3) {
    case 0: 
      genereerEntity(1, SCHATMUNT);
      break;
    case 1:
      genereerEntity(1, SCHATMUNTSTAPEL);
      break;
    case 2:
      genereerEntity(1, SCHATGEM);
      break;
    case 3:
      genereerEntity(1, SCHATKIST);
      break;
    }
  }
}

void genereerRuimteMonsters(int hoeveel) {
  for (int i = 0; i < hoeveel; i++) {
    int random3 = int(random(2));
    switch(random3) {
    case 0: 
      genereerEntity(1, RUIMTEMONSTERLEFTRIGHT);
      break;
    case 1:
      genereerEntity(1, RUIMTEMONSTERUPDOWN);
      break;
    }
  }
}

void genereerEntity(int hoeveel, int entity) {
  for (int i = 0; i < hoeveel; i++) {
    int random1 = int(random(speelveld[level].length)); 
    int random2 = int(random(speelveld[level][1].length)); 
    // if conditie om levels zo veel mogelijk haalbaar te maken
    if (speelveld[level][random1][random2] == LEGERUIMTE && (random1 != 1 || random2 != 1) && random1 != 2 && random2 != 2  && random1 != 0 && random2 != 0 && random1 != speelveld[level].length - 1 && random2 != speelveld[level][1].length - 1 && random1 != speelveld[level][1].length - 2 && random2 != speelveld[level].length - 3 && random1 + random2 != 1 + speelveld[level].length - 3 && random1 + random2 != speelveld[level].length - 2 + speelveld[level].length - 3) {
      speelveld[level][random1][random2] = entity;
    } else { 
      i --;
    }
  }
}

void resetSpel() {
  level = 0;
  score = 0;
}

void resetSpeelveld() {
  for (int i = 1; i <speelveld[level].length - 1; i++) {
    for (int j = 1; j <speelveld[level][i].length - 1; j++) {
      if (speelveld[level][i][j] != LEGERUIMTE && speelveld[level][i][j] != KNOP && level > 0) {
        speelveld[level][i][j] = LEGERUIMTE;
      } else if (speelveld[level][i][j] != LEGERUIMTE && speelveld[level][i][j] != KNOP && speelveld[level][i][j] != MUUR) {
        speelveld[level][i][j] = LEGERUIMTE;
      }
    }
  }
}

void tekenEntity(int x, int y, int index) {
  sprites[index].resize(breedte, hoogte);
  image(sprites[index], x*breedte, y*hoogte);
}

void veranderRichtingRuimteMonster() {
  if (ruimteMonsterWachtTijd % (ruimteMonsterSpeed * 5) == 0) {
    switch(ruimteMonsterRichting) {
    case NAARLINKS:
      ruimteMonsterRichting = NAARRECHTS;
      break;
    case NAARRECHTS:
      ruimteMonsterRichting = NAARLINKS;
      break;
    }
    switch(ruimteMonsterRichting2) {
    case NAARBOVEN:
      ruimteMonsterRichting2 = NAARBENEDEN;
      break;
    case NAARBENEDEN:
      ruimteMonsterRichting2 = NAARBOVEN;
      break;
    }
  }
}

void beweegRuimteMonster() {
  veranderRichtingRuimteMonster();
  if (ruimteMonsterWachtTijd % ruimteMonsterSpeed == 0) {    
    for (int i = 1; i < speelveld[level].length; i++) {
      for (int j = 1; j < speelveld[level][i].length; j++) {
        if (speelveld[level][i][j] == RUIMTEMONSTERLEFTRIGHT && checkOfMonsterErHeenKan(i - 1, j) && ruimteMonsterRichting == NAARLINKS) {     
          speelveld[level][i - 1][j] = RUIMTEMONSTERLEFTRIGHT;
          speelveld[level][i][j] = LEGERUIMTE;
        } else if (speelveld[level][i][j] == RUIMTEMONSTERUPDOWN && checkOfMonsterErHeenKan(i, j - 1) && ruimteMonsterRichting2 == NAARBOVEN) {     
          speelveld[level][i][j - 1] = RUIMTEMONSTERUPDOWN;
          speelveld[level][i][j] = LEGERUIMTE;
        }
      }
    }
    for (int i = speelveld[level].length - 2; i > 0; i--) {
      for (int j = speelveld[level][i].length - 2; j > 0; j--) {
        if (speelveld[level][i][j] == RUIMTEMONSTERLEFTRIGHT && checkOfMonsterErHeenKan(i + 1, j) && ruimteMonsterRichting == NAARRECHTS) {
          speelveld[level][i + 1][j] = RUIMTEMONSTERLEFTRIGHT;
          speelveld[level][i][j] = LEGERUIMTE;
          //println(i);
        } else if (speelveld[level][i][j] == RUIMTEMONSTERUPDOWN && checkOfMonsterErHeenKan(i, j + 1) && ruimteMonsterRichting2 == NAARBENEDEN) {                 
          speelveld[level][i][j + 1] = RUIMTEMONSTERUPDOWN;
          speelveld[level][i][j] = LEGERUIMTE;
        }
      }
    }
  }
}

void beweegIntelligentRuimteMonster() {
  ;
  if (ruimteMonsterWachtTijd % intelligentRuimteMonsterSpeed == 0) {
    for (int i = 1; i < speelveld[level].length; i++) {
      for (int j = 1; j < speelveld[level][i].length; j++) {
        if (speelveld[level][i][j] == INTELLIGENTRUIMTEMONSTER) {
          if (i * breedte > spelers[0][0] * breedte && checkOfMonsterErHeenKan(i - 1, j)) {        
            speelveld[level][i - 1][j] = INTELLIGENTRUIMTEMONSTER;
            speelveld[level][i][j] = LEGERUIMTE;
          } else if (j * hoogte > spelers[0][1] * hoogte && checkOfMonsterErHeenKan(i, j - 1)) {
            speelveld[level][i][j - 1] = INTELLIGENTRUIMTEMONSTER;
            speelveld[level][i][j] = LEGERUIMTE;
          }
        }
      }
    }
    for (int i = speelveld[level].length - 2; i > 0; i--) {
      for (int j = speelveld[level][i].length - 2; j > 0; j--) {
        if (speelveld[level][i][j] == INTELLIGENTRUIMTEMONSTER) {
          if (i * breedte < spelers[0][0] * breedte && checkOfMonsterErHeenKan(i + 1, j)) {
            speelveld[level][i + 1][j] = INTELLIGENTRUIMTEMONSTER;
            speelveld[level][i][j] = LEGERUIMTE;
            //println(i);
          } else if (j * hoogte < spelers[0][1] * hoogte && checkOfMonsterErHeenKan(i, j + 1)) {
            speelveld[level][i][j + 1] = INTELLIGENTRUIMTEMONSTER;
            speelveld[level][i][j] = LEGERUIMTE;
          }
        }
      }
    }
  }
}

boolean checkOfMonsterErHeenKan(int x, int y) {
  return speelveld[level][x][y] == LEGERUIMTE || speelveld[level][x][y] == SCHATMUNT || speelveld[level][x][y] == SCHATMUNTSTAPEL || speelveld[level][x][y] == SCHATGEM || speelveld[level][x][y] == SCHATKIST;
}
